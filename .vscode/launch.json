{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: FastAPI",
			"type": "python",
			"request": "launch",
			"module": "uvicorn",
			"args": ["main:app", "--reload", "--host", "0.0.0.0", "--port", "8080"],
			"justMyCode": false,
			"env": {
				"PYTHONPATH": "${workspaceFolder}"
			}
		},
	]
}
